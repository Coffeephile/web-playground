<!doctype html>
<html>
  <head>
    <meta charset="utf-8">
    <title><%= title %></title>
    <% if (cssBase) { %>
      <link rel="stylesheet" href="<%= cssBase %>.css">
    <% } %>
    <% stylesheets.forEach(function (href) { %>
      <link rel="stylesheet" href="<%= href %>">
    <% }); %>
    <style><%- css %></style>
  </head>
  <body>

    <%- html %>

    <% scripts.forEach(function (src) { %>
      <script src="<%= src %>"></script>
    <% }); %>
    <script><%- js %></script>
  </body>
</html>
